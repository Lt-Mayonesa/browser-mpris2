{
  "source": "./src",
  "destination": "./docs",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "unexportedIdentifier": {
          "enable": true
        },
        "manual": {
          "index": "./manual/index.md",
          "files": [
            "./manual/how_to.md",
            "./manual/youtube.md",
            "./manual/youtube-music.md",
            "./manual/netflix.md",
            "./manual/soundcloud.md"
          ]
        },
        "test": {
          "source": "./tests/",
          "interfaces": [
            "describe",
            "it"
          ],
          "includes": [
            "(spec|Spec)\\.js$"
          ],
          "excludes": [
            "utils/",
            "\\.config\\.js$"
          ]
        }
      }
    },
    {
      "name": "esdoc-ecmascript-proposal-plugin",
      "option": {
        "all": true
      }
    }
  ]
}
