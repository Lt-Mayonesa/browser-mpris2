{
  "source": "./src",
  "destination": "./docs",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "unexportedIdentifier": {
          "enable": true
        },
        "manual": {
          "index": "./manual/index.md",
          "files": [
            "./manual/how_to.md",
            "./manual/youtube.md",
            "./manual/netflix.md",
            "./manual/soundcloud.md"
          ]
        }
      }
    },
    {
      "name": "esdoc-ecmascript-proposal-plugin",
      "option": {
        "all": true
      }
    }
  ]
}
