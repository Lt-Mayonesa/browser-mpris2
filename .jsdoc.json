{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc"
    ]
  },
  "source": {
    "include": [
      "README.md"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs|tests|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "referenceTitle": "Browser Mpris2",
    "disableSort": true,
    "collapse": false,
    "resources": {
      "Content Scripts": "https://developer.chrome.com/extensions/content_scripts",
      "otommod/browser-mpris2": "https://github.com/otommod/browser-mpris2"
    },
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "showInheritedInNav": true
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "/home/joaco/.jsdoc/jsdoc-template"
  }
}
